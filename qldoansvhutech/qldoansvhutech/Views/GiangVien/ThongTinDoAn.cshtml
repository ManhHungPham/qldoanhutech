@model IEnumerable<qldoansvhutech.Models.doan>

@{
    ViewBag.Title = "ThongTinDoAn";
    Layout = "~/Views/Shared/LayoutGV.cshtml";
}

<table class="table">
    <tr>
        <th>
            Loại Đồ Án
        </th>
        <th>
            Thời gian bắt đầu
        </th>
        <th>
            Thời gian kết thúc
        </th>
        <th>Tùy chọn</th>
    </tr>

@foreach (var item in Model) {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.tenda)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.thoigianbd)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.thoigiankt)
    </td>
    <td>
        <a href="#" class="btn btn-info btn-sm" data-toggle="modal" data-target="#modal_edit_user_5"><i class="fa fa-pencil" aria-hidden="true"></i>Sửa</a>
        <a href="#" class="btn btn-danger btn-sm" data-toggle="modal" data-target="#modal_edit_user_5"><i class="fa fa-trash-o" aria-hidden="true"></i>Xóa</a>

    </td>

</tr>
}
</table>
